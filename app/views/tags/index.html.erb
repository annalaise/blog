<div class="card">
  <div class="card-body">
    <h4 class="card-title">Tags</h4>

      <ul class="list-group list-group-flush">
        <% @tags.each do |tag| %>
        <li class="list-group-item">
          <%= link_to tag.name, tag_path(tag) %>
        </li>
        <% end %>
      </ul>

  </div>
</div>
